{
    "name": "mystflip/location",
    "description": "Retrieve a users location by their IP Address",
    "authors": [
        {
            "name": "Phil Pasquin",
            "email": "p@pasquin.ca"
        }
    ],
    "license" : "MIT",
    "keywords" : [
        "statamic",
        "php",
        "location",
        "geo",
        "geoIP",
        "geo-location",
        "ip"
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0",
        "geoip2/geoip2": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9|^1.0",
        "orchestra/testbench": "~3.2|~4.0|^6.0"
    },
    "archive": {
      "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Mystflip\\Location\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mystflip\\Location\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mystflip\\Location\\LocationServiceProvider"
            ],
            "aliases": {
                "Location": "Mystflip\\Location\\Facades\\Location"
            }
        }
    }
}
